body{
	background-color: #f2f2f2;
	overflow: scroll;
	height: 100%;
-moz-box-shadow: 0px 2px 2px rgba(0,0,0,0.1);
-webkit-box-shadow: 0px 2px 2px rgba(0,0,0,0.1);
box-shadow: 0px 2px 2px rgba(0,0,0,0.1);	
	
}
html{
	background-color: #246390;
}
html, body{
	font-family: Helvetica Nueue, sans-serif;
}
#footer-wrapper{
}
#footer-text{
	text-align: center;
	font: bold 1.1em Arial, sans-serif;
	text-shadow: 0 -1px 0 rgba(0,0,0,0.5);
	color: rgba(255,255,255, 0.75);
	width: 950px;
	margin: 30px auto -100px auto;
}
#site-container{
	width: 950px;
	height: 100%;
	margin: 0 auto 0 auto;
	overflow: auto;
	box-shadow: 0 0px 3px rgba(0,0,0, 0.2);
	-moz-box-shadow: 0px 0px 13px rgba(0,0,0, 0.2);
	-webkit-box-shadow: 0px 0px 13px rgba(0,0,0, 0.2);
	border: dashed 1px #CCCCCC;
}
#main{
	background-color: #FFFFFF;
	top: 0px;
	right: 0px;
	bottom: 0px;
	left: 0px;
	height: 100%;
	margin: 0 auto 0 auto;
	overflow: auto;
	padding-bottom: 25px;
	border-top: dashed 1px #CCCCCC;

	
}
#page{
	margin: 0 auto;
	padding: 15px;
	height: 100%;

}
.navbar .divider-vertical {
  width: 1px;
  height: 40px;
  margin: 12px 9px;
  overflow: hidden;
  background-color: rgba(255,255,255,0.5);
  border-right: 1px solid rgba(0,0,0,0.5);
}
.sensitive{
	color: transparent;
	text-shadow: 0 0 10px black;
	-webkit-transition: all .1s linear;
}
.sensitive:hover{
	color: inherit;
	text-shadow: none;
}
.alignCenter{
	text-align:center;
}

.clean{
	list-style: none;
}
#header{
	margin: auto;
	width: 950px;
}
span.header{
	font-size: 1.2em;
}
span.footer{
	color: #404040;
	text-align: center;
	font-size: 1.2em;
}
h1{
	color:#616158;
}
h2{
	 color:#404040;
}
h3{
	color: #0070C0;
	font-size: 1.4em;
}
h4{
	color: #404040;
	font-size: 1.1em;
	margin-bottom: 0px;
}
#brand{
	-webkit-transition-duration: 0.30s;
	-webkit-transition-timing-function: ease-out;
	-moz-transition-duration: 0.30s;
	-moz-transition-timing-function: ease-out;	
	display: block;
	font-size: .9em;
	float: left;
	padding-top: 20px; 
	padding-right: 20px; 
	padding-left: 20px;
}
#navigation{
	display: block;
	height: 70px;
	background: -moz-linear-gradient(#FAFAFA, #FFFFFF);
	background: -webkit-linear-gradient(#FAFAFA, #FFFFFF);
}
.navigation{
	list-style-type: none;
	position: relative;
	left: 0;
	display: block;
	float: left;
	margin: 10px 10px 0 0;
}
.navigation li{
	display: block;
	float: left;
}
.navigation li a{
	text-align: center;
	-webkit-transition-duration: 0.30s;
	-webkit-transition-timing-function: ease-out;
	-moz-transition-duration: 0.30s;
	-moz-transition-timing-function: ease-out;	
	text-transform: Uppercase;
	font-size: 1.1em;
	font-family: Open Sans, sans serif;
	display: block;
	width: 100px;
	height: 50px;
	border-left: solid 1px #CCCCCC;
}
.navigation li:last-child{
	border-right: solid 1px #CCCCCC;
}
.navigation li a:hover{
	width: 130px;
	background-color: #4B4543; 
	color: #FFFFFF;
	border-bottom: solid 3px #FF4500;
	text-decoration: none;
}
.navigation li a:active{
	color: #ff4500;
}
#brand:hover{
	text-decoration: none;
}
#brand:hover .name1{
	color: #FF4500;
	text-shadow: 0 0 0.1em #FF4500;
}
#brand:hover .name2{
	color: #0070C0;
	text-shadow: 0 0 0.1em #0070C0;
}
.navigation li a:hover .info{
	display: block;
}
.navigation .info{
	display: none;
	font-size: .7em;
	text-transform: none;
	font-style: italic;
}
